{
  "name": "Gmail.js",
  "version": "0.1",
  "description": "Hello World Gmail chrome extension",
    
  "browser_action": {
    "default_title": "Open the popup",
    "default_popup" : "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    "jquery-1.10.2.min.js",
    "gmail.js",
    "main.js"
  ],
  "manifest_version": 2
}
